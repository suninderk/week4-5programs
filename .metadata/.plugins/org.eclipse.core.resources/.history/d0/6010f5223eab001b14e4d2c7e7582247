package week4;

import acm.program.ConsoleProgram;
import acm.program.*;
import java.util.*;
public class UniqueNames extends ConsoleProgram {
	
	public void run() {
		ArrayList<String> names = new ArrayList<String>();
while (true) {
	String name=readLine("Enter name : ");
	if (names.equals("")) {
		break;
	}
	else { names.add(name);
		
	}
		
	}
	
}
		
	}
	
}
